    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "typescript": "^4.7.4",
    "web-vitals": "^2.1.4",
    "webpack": "^5.73.0",
    "webpack-dev-server": "^4.9.3",
    "webpack-manifest-plugin": "^4.0.2",
    "workbox-webpack-plugin": "^6.5.3"
  }
}
